      "remote": {
        "candid": "did/internet_identity.did",
        "id": {
          "ic": "rdmx6-jaaaa-aaaaa-aaadq-cai",
          "playground": "rdmx6-jaaaa-aaaaa-aaadq-cai"
        }
      },
      "frontend": {},
      "declarations": {
        "output": "./declarations/internet_identity"
      }
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": "mops sources"
    }
  },
  "dfx": "0.22.0",
  "version": 1,
  "output_env_file": ".env"
